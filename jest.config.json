{
  "collectCoverageFrom": ["src/**/*.js"],
  "coverageReporters": ["lcov", "text"],
  "moduleNameMapper": {
    "\\.(css|scss)$": "identity-obj-proxy",
    "\\.(pdf)$": "<rootDir>/mocks/file-mock.js"
  },
  "setupFiles": ["./test-setup.js"],
  "testMatch": ["**/?(*.)+(test).js"],
  "testPathIgnorePatterns": ["<rootDir>/node_modules/"],
  "transform": {
    "^.+\\.js$": "babel-jest"
  }
}
